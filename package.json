{
  "name": "ypp-pathways-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/maybe-db-sync.mjs && rm -rf .next node_modules/.prisma && prisma generate && next build",
    "vercel-build": "npm run build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "smoke:release": "node scripts/smoke-release-checks.mjs",
    "check:release": "npm run build && npm run nav:check && npm run smoke:release",
    "nav:check": "node scripts/validate-nav.mjs",
    "nav:ux-check": "node scripts/validate-nav.mjs",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "backfill:instructor-readiness": "node scripts/backfill-native-instructor-readiness.mjs",
    "backfill:passion-taxonomy": "node scripts/backfill-passion-taxonomy.mjs",
    "training:seed-content": "node scripts/seed-training-academy-content.mjs",
    "training:validate": "node scripts/validate-training-academy-content.mjs",
    "training:import": "node scripts/import-training-academy-content.mjs",
    "training:export": "node scripts/export-training-academy-content.mjs",
    "training:sync": "npm run training:validate && npm run training:import"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^5.18.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@upstash/ratelimit": "^2.0.8",
    "@vercel/blob": "^2.2.0",
    "@vercel/kv": "^3.0.0",
    "bcryptjs": "2.4.3",
    "clsx": "2.1.1",
    "next": "14.2.28",
    "next-auth": "4.24.13",
    "nodemailer": "^7.0.13",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "^6.9.1",
    "three": "^0.170.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^25.2.3",
    "@types/nodemailer": "^7.0.9",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/three": "^0.170.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "jsdom": "^28.1.0",
    "prisma": "^5.18.0",
    "tsx": "4.15.6",
    "typescript": "5.5.4",
    "vitest": "^4.0.18"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
